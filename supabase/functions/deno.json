{
  "imports": {
    "npm:@supabase/supabase-js": "npm:@supabase/supabase-js@2.39.3",
    "npm:stripe": "npm:stripe@17.7.0",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@2.39.3",
    "stripe": "npm:stripe@17.7.0"
  },
  "compilerOptions": {
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "lib": ["deno.ns", "dom", "dom.iterable", "esnext"],
    "types": ["npm:@types/stripe"]
  },
  "importMap": "./import_map.json",
  "lint": {
    "include": ["**/*.ts"],
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "no-unused-vars"]
    }
  },
  "fmt": {
    "files": {
      "include": ["**/*.ts"]
    },
    "options": {
      "useTabs": false,
      "lineWidth": 100,
      "indentWidth": 2,
      "singleQuote": true
    }
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read supabase/functions/stripe-webhook/index.ts"
  }
} 