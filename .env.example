# Standard environment variables
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_ASKJDS_DOMAIN=askjds.com
VITE_JDSIMPLIFIED_DOMAIN=jdsimplified.com
VITE_ADMIN_DOMAIN=admin.jdsimplified.com
BUILD_DOMAIN=askjds|jds|admin

# Google AI API Key
GOOGLE_AI_API_KEY=your-google-api-key

# AI Model Configuration 
# These variables control which AI models are used for different tasks
# The actual model names are obfuscated for security - these are code names
# Development environment
AI_MODEL_PRIMARY_DEV=jds-titan    # For main chat responses
AI_MODEL_SECONDARY_DEV=jds-flash  # For thread titles
# Production environment
AI_MODEL_PRIMARY_PROD=jds-titan   # For main chat responses
AI_MODEL_SECONDARY_PROD=jds-flash # For thread titles
# Logging configuration
AI_MODELS_LOGGING=false           # Set to true in development for detailed logs

# Stripe configuration
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_TEST_WEBHOOK_SECRET=your-stripe-webhook-secret 

# Production Environment Configuration
# Copy this file to .env and fill in your values

# Application Mode
NODE_ENV=production
VITE_DEBUG_MODE=false

# API Keys (Keep these secret!)
VITE_OPENAI_API_KEY=your_openai_api_key
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
VITE_STRIPE_SECRET_KEY=your_stripe_secret_key

# Price IDs for different tiers
VITE_STRIPE_ASKJDS_UNLIMITED_MONTHLY_PRICE_ID=price_xxx
VITE_STRIPE_ASKJDS_UNLIMITED_ANNUAL_PRICE_ID=price_xxx
VITE_STRIPE_ASKJDS_PREMIUM_MONTHLY_PRICE_ID=price_xxx
VITE_STRIPE_ASKJDS_PREMIUM_ANNUAL_PRICE_ID=price_xxx

# Analytics Configuration
VITE_USERMAVEN_KEY=your_usermaven_key
VITE_GA_MEASUREMENT_ID=your_ga_measurement_id

# Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn
VITE_SENTRY_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DEBUG_UI=false
VITE_ENABLE_ADMIN_FEATURES=false

# Security Configuration
VITE_ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com
VITE_CSP_REPORT_URI=https://yourdomain.com/api/csp-report

# Performance Configuration
VITE_API_TIMEOUT=30000
VITE_REALTIME_RECONNECT_DELAY=1000
VITE_MAX_RECONNECT_ATTEMPTS=10

# Cache Configuration
VITE_CACHE_TTL=3600000
VITE_ENABLE_SERVICE_WORKER=true 