# Ask-JDS Environment Template
# Copy this file to .env and customize as needed

# Build configuration
VITE_BUILD_DOMAIN=askjds
# Options: askjds, jdsimplified, admin

# Supabase configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Feature flags
VITE_ENABLE_OPENAI=false
VITE_ENABLE_GEMINI=false
VITE_ENABLE_TOOLS_PANEL=true
VITE_ENABLE_PAYMENTS=false
VITE_ENABLE_DATA_SYNC=true

# External service API keys (leave empty if not using)
VITE_OPENAI_API_KEY=
VITE_GEMINI_API_KEY=
VITE_STRIPE_PUBLISHABLE_KEY=
VITE_STRIPE_SECRET_KEY=

# Application settings
VITE_APP_NAME=Ask JDS
VITE_DEFAULT_LOCALE=en-US
VITE_LOG_LEVEL=info

# Performance settings
VITE_ENABLE_QUERY_CACHE=true
VITE_QUERY_STALE_TIME=300000
VITE_MAX_CONCURRENT_REQUESTS=3 